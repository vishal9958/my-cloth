{
  "expo": {
    "name": "My Cloth",
    "slug": "my-cloth",
    "version": "1.0.0",
    "orientation": "portrait",
    "//": "2. Android Settings Fixed",
    "icon": "./assets/images/icon.png",
    "scheme": "mycloth",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "//": "Foreground ko bhi main icon.png par point kar diya taki error na aaye",
        "backgroundColor": "#0f0c29",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.vishal00.mycloth"
    },
    "web": {
      "output": "static",
      "//": "3. Web Favicon (Transparent wala)",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "//": "Main background color ko dark kiya",
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0f0c29",
          "dark": {
            "backgroundColor": "#0f0c29"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bde44484-9982-4886-94c8-b8d5eb2d3609"
      }
    }
  }
}
